{
  "extends": [
    "standard",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react",
    "prettier/standard"
  ],
  "plugins": ["react", "jsx-a11y", "prettier", "standard", "jasmine"],
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "node": true,
    "jasmine": true
  },
  "rules": {
    "prettier/prettier": "error"
  }
}
